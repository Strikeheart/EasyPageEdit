{
	"manifest_version": 2,
	"name": "Easy page editor",
	"description": "Gestalten Sie Ihre Lieblingsseite selbst",
	"version": "0,1",
	"icons": {
		"128": "../images/128.png"
	},
	"permissions": [
		"tabs",
		"http://*/*",
		"background"
	],
	"content_scripts": [
		{
			"matches":  ["http://*/*"], 
			"js" : ["inject.js"] 
		}
	]
	"browser_action": { "default_icon": "../images/16.png", "popup",: "popup.html"
}